/* pages/Schedule/Schedule.wxss */
#map{
  width: 100%;
  height: 100%;
}
.contarn{
  padding: 20rpx;
}
.hotel{
  padding-top:90rpx; 
  width: 100%;
  height: 480rpx;
}
.box{
  font-size: 12px;
  color: #888;
  line-height: 15px;
}
.h1{
  font-size: 14px;
  color: rgb(24, 157, 160);
}
.pink{
  color: #de0051;
}
.tr{
  width: 100%;
  vertical-align: top;
}
.table{
  margin-top:20rpx;
  margin-bottom: 15rpx;
  border-left: 1rpx solid #d4d4d4;
}
.th{
  width: 235rpx;
  display: inline-block;
  color: #fff;
  background: rgb(24, 157, 160);
  text-align: center;
  height: 20px;
  line-height: 20px;
  border-right: 1rpx #fff solid;
}
.td{
  width: 235rpx;
  display: inline-block;
  text-align: center;
  height: 100%;
  vertical-align: top;
  line-height: 20px;
  border-right: 1rpx solid #d4d4d4;
  border-bottom: 1rpx solid #d4d4d4;
}
.tdhei{
  height: 40px;
  line-height: 40px;
}
.header{
  margin:20rpx;
  padding: 20rpx;
  color: #444;
  font-size: 16px;
  border-left:5px solid rgb(24, 157, 160);
}