/* pages/Schedule/Schedule.wxss */
.paio{
  margin: 0px auto;
  margin-top: 30rpx;
  width: 90%;
  border-radius: 10rpx;
  border:1rpx solid #9f9fa0;
  background: #fefefe;
}
.paiosele{
  margin: 0px auto;
  margin-top: 30rpx;
  width: 90%;
  border-radius: 10rpx;
  border:1rpx solid #00a29a;
  background: #fefefe;
}
.contern{
  padding: 20rpx;
}
.title{
  font-size: 16px;
  color: #333;
}
.price{
  font-size: 16px;
  color: #00a29a;
}
.line{
  margin-top:15rpx;
  border-top: 1rpx #f0f0f0 solid;
  width: 100%;
  height: 1rpx;
}
.subcon{
  font-size: 10px;
  color: #888;
}
.spanco{
  color: #00a29a;
}
.form{
  margin: 0px auto;
  margin-top: 10rpx;
  padding-bottom: 100rpx;
  width: 90%;
}
.label{
  font-size: 14px;
}
.int{
  margin: 10rpx 0rpx;
  border: 1rpx solid #d7d7d7;
  background: #fff;
  border-radius: 10rpx;
  height: 70rpx;
}
.red{
  color: #c30d22;
}
.yellow{
  color:#f39700;
}
.submit{
  position: fixed;
  width: 100%;
  bottom: 0rpx;
  height: 85rpx;
  border-top:1rpx solid #f0f0f0; 
  border-radius: 0rpx !important;
  background: #fff;
  color: #00a29a;
  line-height: 85rpx;
  text-align: center;
  z-index: 1000;
}
